	<c:set var="a" value="hello"/>
	<c:set var="b" value="${param.test}"/>
	<c:set var="c" value="${param.a+param.b}"/>
	<c:set var="d" value="${ c+10 }"/>
	<c:set var="d" value="${ d+55 }"/>
	
	${ a } , ${ b } , ${ c } , ${ d }
	<hr><br>
	<c:if test="${ d>10 }">
		d는 10보다 크다
	</c:if>
