package Programers;

import java.util.Vector;

public class Kim {

	public static String solution(Vector<String> seoul) {
		int x = 0;
		for (int i = 0; i < seoul.size(); i++) {
			if ((seoul.get(i).equals("Kim")))
				x = i;
		}

		return "김서방은 " + x + "에 있다";

	}
	
	public static void main(String[] args) {
		
		Vector<String> seoul = new Vector<>();
		seoul.add("Jane");
		seoul.add("Kim");
		
		
		System.out.println(solution(seoul));
		
		
	}
}
