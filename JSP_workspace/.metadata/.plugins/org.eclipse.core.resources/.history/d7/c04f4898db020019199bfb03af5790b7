package Programers;

import java.util.Vector;

public class Kim {

	public static String seobang(String[] seoul) {
		
		String result = null;
		for (int i = 0; i < seoul.length; i++) {
			if ((seoul[i].equals("Kim")))
				result = "김서방은 "+ i +"에 있다";
		}

		return result;

	}
	
	public static void main(String[] args) {
		
		String[] seoul = {"Jane","Jane","Jane","Kim"};
		
		System.out.println(seobang(seoul));
		
		
	}
}
