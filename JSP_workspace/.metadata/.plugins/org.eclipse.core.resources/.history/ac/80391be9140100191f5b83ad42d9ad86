package algo_test;

public class Bubble_Sort {
	
	public static void bubble(int[] arr) {
		
		for(int i = 0; i < arr.length ; i ++) {
			for(int j = 0 ; j < arr.length-1-i; j ++) {
				
				if(arr[j] > arr[j+1]) {
					int temp = 0;
					temp = arr[j];
					arr[j] = arr[j+1];
					arr[j+1] = temp;
				}
				
			}
		}
		
	}
	
	public static void main(String[] args) {
		
		int[] arr = {3,40,26,69,12,36};
		
		bubble(arr);
		
		for(int num : arr) {
			System.out.println(num + " ");
		}
		
	}
}
