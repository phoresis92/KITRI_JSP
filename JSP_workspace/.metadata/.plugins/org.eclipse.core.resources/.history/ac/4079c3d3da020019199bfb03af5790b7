package Programers;

import java.util.Vector;

public class Kim {

	public static String solution(Vector<String> seoul) {
		
		String result = null;
		for (int i = 0; i < seoul.size(); i++) {
			if ((seoul.get(i).equals("Kim")))
				result = "김서방은 "+ i +"에 있다";
		}

		return result;

	}
	
	public static void main(String[] args) {
		
		Vector<String> seoul = new Vector<>();
		seoul.add("Jane");
		seoul.add("Jane");
		seoul.add("Jane");
		seoul.add("Jane");
		seoul.add("Kim");
		
		
		System.out.println(solution(seoul));
		
		
	}
}
